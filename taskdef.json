{
    "taskRoleArn": "arn:aws:iam::MY_AWS_ACCOUNT_ID:role/levi9-cinema-fe-ecs-service-role",
    "executionRoleArn": "arn:aws:iam::MY_AWS_ACCOUNT_ID:role/levi9-cinema-fe-ecs-task-role",
    "containerDefinitions": [
        {
            "name": "levi9-cinema-fe",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "levi9-cinema-fe"
}
